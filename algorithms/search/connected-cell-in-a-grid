#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
    int m,n,i,j;
    cin>>m>>n;
    vector< vector<int> > v(m, vector<int>(n));
    for (i=0;i<m;i++)
        for (j=0;j<n;j++){
            cin>>v[i][j];
    }
    
    int ja, jz,t,k;
    for (i=0;i<m;i++){
        ja=n+2; jz=0; t=0;
        for (j=0;j<n;j++){
            if (v[i][j]!=0 && j!=n-1){
                t+=v[i][j];
                if (ja==n+2) ja=j;
                else jz=j;
            }
            else {
                if (j==n-1) {jz=j; t++;}
                //cout <<"XXXX"<<" i="<<i<<" j="<<j<<" ja="<<ja<<" jz="<<jz<<endl;
                for (k=jz;k>=ja;k--){
                    v[i][k]=t;
                }
                ja=n+2; jz=0; t=0;
            }
        }
    }
/*    
    for (i=0;i<m;i++){
        for (j=0;j<n;j++){
            cout<<v[i][j]<<" ";
        }
        cout<<endl;
    }
*/
    int c=0, Max=0;
    for (j=0;j<n;j++){
        if(v[i][j]!=0) c+=v[i][j];
        if()
    }
    
    
    
    return 0;
}

